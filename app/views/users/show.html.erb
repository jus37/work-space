<div class="header">
  <%= render "devise/shared/header" %>
</div>
<div class="user-show">
  <div class="breadcrumbs">
    <% breadcrumb :user%>
    <%= render "layouts/breadcrumb"%>
  </div>
  <div class="user">
    <div class="user-header">
    <div class="user-tab">
      <%= render "users/header"%>
    </div>
    <div class="user-info">
      <div class="user-info-top">
        <p>アカウント情報</p>
      </div>
      <div class="user-info-content">
    <%= "#{@user.email}"%>
    <%= "#{@user.telephone}"%>
    <%= "#{@user.comment}コメント"%>
</div>
<div class="footer">
  <%= render "devise/shared/footer" %>
</div>
